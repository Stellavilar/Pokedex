<%- include('header') %>

<% for(let pokemon of pokemons) { %>
    <div>
        <a class="thumbnail" href="/pokemon/<%= pokemon.numero %>">
            <img src="/img/<%= pokemon.numero %>.png" alt="Illustration de <%= pokemon.nom %>">
            <span># <%= pokemon.numero %> <%= pokemon.nom %></span>
        </a>
    </div>
<% } %>

<%- include('footer') %>